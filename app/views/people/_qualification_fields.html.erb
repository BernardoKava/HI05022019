

<div class="nested-fields">
  <div class="jumbotron"style="background-color: bisque;color: navy">
    <div class="form-inputs">
      <%= f.input :name %>
      <%= f.input :school %>
      <%= f.input :from , :start_year =>2000, :end_year => 2030 %>
      <%= f.input :to,:start_year =>2000, :end_year => 2030%>
      <%= f.input :grade %>
      <%= f.input :user_id ,:as => :hidden, :input_html => { :value => current_user.id}%>
      <%= f.input :active %>
    </div>

  <div class="breadcrumb"style="background-color: khaki">
    <%= link_to_remove_association 'Remove', f ,class: 'fa fa-close', style:"font-size:15px;color:red"%>
  </div>

  </div>



</div>
